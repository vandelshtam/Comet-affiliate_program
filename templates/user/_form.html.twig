{% if form_errors(form) %}
<div class="my-custom-class-for-errors">
    {{ form_errors(form) }}
</div>
{% endif %}
{{ form_start(form) }}
<div class="form_box-inner">
    <div class="form_box-left">
    {{ form_row(form.username) }} 
    {{ form_row(form.email) }} 
    {# {{ form_row(form.phone) }} 
    {{ form_row(form.state) }} 
    {{ form_row(form.region) }} 
    {{ form_row(form.city) }} 
    {{ form_row(form.indexcity) }} 
    {{ form_row(form.street) }} 
    {{ form_row(form.house) }} 
    {{ form_row(form.frame) }} 
    {{ form_row(form.apartment) }}  #}
    <button class="btn btn-warning">{{ button_label|default('Сохранить') }}</button>
    </div>
</div>
{{ form_end(form) }}
