{% block javascripts_three %}
 
<script>
      var chart = bb.generate({
        data: {
          columns: [
            ["Blue USDT", {{ wallet.usdt }}],
            ["Orange CoMetaCoin", {{ wallet.cometpoin }}/{{ token_rate }}],
            ["Green Etherium", {{ wallet.etherium }} * 4000],
            ["Red Bitcoin", {{ wallet.bitcoin }} * 40000],  
          ],
          type: "donut",
          onclick: function (d, i) {
            console.log("onclick", d, i);
          },
          onover: function (d, i) {
            console.log("onover", d, i);
          },
          onout: function (d, i) {
            console.log("onout", d, i);
          },
        },
        donut: {
          title: "100%",
        },
        bindto: "#donut-chart",
      });
</script>
{% endblock %}